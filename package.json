{
  "name": "typescript-express-nodemon-minimal-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r ./src/server.ts",
    "debug": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "build": "tsc"
  },
  "author": "Michael Gradek <https://www.youtube.com/channel/UCp2nLiwdRWfbVyWimngmFfw>",
  "license": "ISC",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "csv-parser": "^3.0.0",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.3",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts"
  }
}
